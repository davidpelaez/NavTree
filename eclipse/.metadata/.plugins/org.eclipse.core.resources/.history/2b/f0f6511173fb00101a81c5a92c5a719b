package navtree;
import processing.core.*;
import processing.opengl.*;
import controlP5.*;
import peasy.*;
import org.json.*; 

public class Sketch extends PApplet {

  public void setup() {
    size(200,200);
    background(0);
  }

  public void draw() {
    stroke(255);
    if (mousePressed) {
      line(mouseX,mouseY,pmouseX,pmouseY);
    }
  }
}
