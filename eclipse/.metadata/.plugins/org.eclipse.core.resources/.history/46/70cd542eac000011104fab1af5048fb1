package navtree;

import controlP5.ControlEvent;
import processing.core.*;
import peasy.*;

public class SimpleTree2D extends PApplet {
	
	Navtree navtree;
	 

	public void setup() {
		size(1000, 400);
		frameRate(12);
		navtree = new Navtree(this);
		smooth();
	}

	public void draw() {
		background(255);
		navtree.draw();
	}

	

}
